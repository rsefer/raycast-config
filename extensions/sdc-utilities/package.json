{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sdc-utilities",
  "title": "SDC Utilities",
  "description": "Sefer Design Co. utilities",
  "icon": "logo_square_rounded.svg",
  "author": "rsefer",
  "owner": "sefer-design-co",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "window-test",
      "title": "SDC Window Test",
      "description": "Window testing script",
      "mode": "no-view"
    },
    {
      "name": "ai-test",
      "title": "SDC AI Test",
      "description": "AI testing script",
      "mode": "no-view"
    },
    {
			"name": "latin-paragraph",
      "title": "Generate Latin Paragraph",
      "description": "Generate a random Latin paragraph (lorem ipsum) and paste it into the focused application",
      "mode": "no-view",
			"keywords": ["latin", "lorem", "ipsum"]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.6",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "25.2.3",
    "@types/react": "19.2.14",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
